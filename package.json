{
  "name": "websokets-mongo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:production": "npm run lint && npm run build:client:production && npm run build:server:production",
    "build:client": "NODE_ENV=development webpack --config scripts/webpack/config.js -d",
    "build:server": "NODE_ENV=development ./node_modules/.bin/babel -d build/server src/server",
    "build:client:production": "NODE_ENV=production webpack --config scripts/webpack/config.js -d",
    "build:server:production": "NODE_ENV=production ./node_modules/.bin/babel -d build/server src/server",
    "dev:client": "NODE_ENV=development webpack-dev-server --config scripts/webpack/config.js --hot --content-base build",
    "dev:server": "NODE_ENV=development nodemon --watch src/server --watch config src/server/index.js --exec babel-node",
    "lint": "eslint src",
    "server": "NODE_ENV=development node build/server",
    "server:production": "NODE_ENV=production node build/server",
    "start": "npm run server:production",
    "dev": "npm run dev:server & npm run dev:client",
    "production": "npm run build:production && npm start",
    "generate:ssl": "nginx/generate-ssl.sh",
    "test:flow": "flow check --show-all-errors src",
    "coverage:flow": "flow-coverage-report"
  },
  "pre-commit": {
    "run": [
      "lint",
      "test:flow"
    ],
    "colors": false,
    "silent": false
  },
  "flow-coverage-report": {
    "includeGlob": [
      "src/**/*.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-flow-runtime": "^0.17.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-async-await": "^0.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.68.0",
    "flow-coverage-report": "^0.5.0",
    "flow-runtime": "^0.17.0",
    "flow-runtime-validators": "^0.17.0",
    "jest": "^22.4.3",
    "json-loader": "^0.5.7",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "nodemon": "^1.17.2",
    "pre-commit": "^1.2.2",
    "process-env": "^1.1.0",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "compression": "^1.7.2",
    "config": "^1.30.0",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "font-awesome": "^4.7.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "mongoose": "^5.0.11",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-draggable": "^3.0.5",
    "react-router-dom": "^4.2.2",
    "request": "^2.85.0",
    "socket.io": "^2.0.4"
  }
}
